	Object o1 = request.getAttribute("num1");
	Object o2 = request.getAttribute("num2");
	Object o3 =application.getAttribute("num3");
	if(o1==null){
		o1= new AtomicInteger(1);	
		request.setAttribute("num1", o1);
	}
	
	if(o2==null){
		o2= new AtomicInteger(1);	
		request.setAttribute("num2", o2);
	}
	
	if(o3==null){
		o3= new AtomicInteger(1);	
