    pageEncoding="UTF-8"%>
<%@ page import="java.util.*"%>
<%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>choose 태그</title>
</head>
<body>
	<ol>
		<c:choose>
			<c:when test="${param.name == 'bk'}">
				<li>당신의 이름은 ${param.name}입니다.</li>
			</c:when>
			<c:when test="${param.age > 20}">
				<li>당신의 20세 이상입니다.</li>
			</c:when>
		</c:choose>
	</ol>
</body>
</html>