	response.addCookie(cookie1);
	
	Cookie cookie2 = new Cookie("my-cookie-7", "my-value-7");
	cookie2.setPath(request.getContextPath());
	cookie2.setHttpOnly(true);
	cookie2.setMaxAge(300);
	response.addCookie(cookie2);
	%>
